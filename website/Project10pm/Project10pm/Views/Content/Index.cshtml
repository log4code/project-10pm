@model Dictionary<int, string?>
@{
}

@Html.ActionLink("Add","Add","Content")


<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Content</th>
        </tr>
    </thead>
    <tbody>
        @{
            int rowNumber = 0;
        }
        @foreach(var item in Model)
        {
            rowNumber = rowNumber + 1;
            <tr>
                <th scope="row">@rowNumber</th>
                <td>@item.Key</td>
                <td>@item.Value</td>
            </tr>
        }
    </tbody>
</table>
